datasource db {
  provider = "postgresql"
}


generator client {
  provider = "prisma-client-js"
}

model Products{
  id          Int    @id @default(autoincrement())
  name        String @unique
  description String
  price       Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  deletedAt   DateTime?
}
